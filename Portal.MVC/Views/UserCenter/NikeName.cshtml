@model Niqiu.Core.Domain.User.User

@{
    ViewBag.Title = "修改昵称";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div id="nikename" class="page">
    @Html.Partial("m_header")
    @Html.Partial("m_footer")
    <div class="content bkmi">
        <div class="list-block">
            <ul>
                 
                <li>
                    <div class="item-content">
                        <div class="item-media"><i class="icon icon-form-email"></i></div>
                        <div class="item-inner">
                            <div class="item-title label">新用户名</div>
                            <div class="item-input">
                                <input id="newname" type="text" placeholder="新用户名">
                            </div>
                        </div>
                    </div>
                </li>
                <li class="content-padded"><a id="nicknamebt" class="button button-fill  button-danger">确定修改</a></li>
            </ul>
        </div>
    </div>
</div>